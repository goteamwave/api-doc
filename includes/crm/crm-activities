<aside>GET api/crm/activities/{activity_id}</aside>

<table><thead>
<tr>
<th>Attribute</th>
<th>Description</th>
</tr>
</thead><tbody>
<tr>
<td>id (<em>integer</em>)</td>
<td>Activity ID</td>
</tr>
<tr>
<td><a href="#activity-type-object">act_type</a></td>
<td>activity type</td>
</tr>
<tr>
<td><a href="#user-object">assigned_to</a></td>
<td>list of people assigned to the activity</td>
</tr>
<tr>
<td><a href="#person-object">person</a></td>
<td>contact person details</td>
</tr>
<tr>
<td><a href="#company-object">company</a></td>
<td>organisation details</td>
</tr>
<tr>
<td><a href="#deals-object">deal</a></td>
<td>deals info</td>
</tr>
<tr>
<td>activity_due_on</td>
<td>due date-time for the activity</td>
</tr>
<tr>
<td>due_time</td>
<td>due time of the activity</td>
</tr>
<tr>
<td>due_date</td>
<td>due date of the activity</td>
</tr>
<tr>
<td>add_next</td>
<td>if the activity added next</td>
</tr>
<tr>
<td>title</td>
<td>title of the activity</td>
</tr>
<tr>
<td>note</td>
<td>description of the activity</td>
</tr>
<tr>
<td>duration</td>
<td>duration of the activity</td>
</tr>
<tr>
<td>is_completed</td>
<td>is the activity completed</td>
</tr>
<tr>
<td>completed_on</td>
<td>date of complete</td>
</tr>
<tr>
<td>is_trashed</td>
<td>is the activity deleted</td>
</tr>
</tbody></table>

