<table><thead>
<tr>
<th>Attribute</th>
<th>Description</th>
</tr>
</thead><tbody>
<tr>
<td>id (<em>integer</em>)</td>
<td>Organization ID</td>
</tr>
<tr>
<td>domain (<em>string</em>)</td>
<td>domain used by the organization</td>
</tr>
<tr>
<td>name (<em>string</em>)</td>
<td>name of organization</td>
</tr>
<tr>
<td>address (<em>string</em>)</td>
<td>address of organization</td>
</tr>
<tr>
<td>phone (<em>string</em>)</td>
<td>phone number</td>
</tr>
<tr>
<td><a href="#user-object">created_by</a> (<em>object</em>)</td>
<td>organization added-by user details</td>
</tr>
<tr>
<td><a href="#user-object">owner</a> (<em>object</em>)</td>
<td>owner details</td>
</tr>
<tr>
<td>created_on (<em>string</em>)</td>
<td>creation date</td>
</tr>
<tr>
<td>people (<em>integer</em>)(<em>integer</em>)</td>
<td>total people added to the organization</td>
</tr>
<tr>
<td>deals (<em>integer</em>)</td>
<td>total deals added</td>
</tr>
<tr>
<td>tags (<em>string</em>)</td>
<td>tags related to organization</td>
</tr>
<tr>
<td><a href="#custom_fields-object">custom_fields</a> (<em>object</em>)</td>
<td>custom fields added to the organization add-form</td>
</tr>
</tbody></table>

